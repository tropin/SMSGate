    <xs:complexType>
      <xs:sequence>
        <xs:element name="login" type="xs:string" />
        <xs:element name="pwd" type="xs:string" />
        <xs:element maxOccurs="unbounded" minOccurs ="1" name="sms_id" type="xs:string" />
      </xs:sequence>
      <xs:attribute name="method" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>
</xs:schema>